<statement>                 ::= <output>"="<expr>";"<newline>
<output>                    ::= "out"
<expr>                      ::= "(("<expr>")&("<expr>"))" | "(("<expr>")|("<expr>"))" | "(!("<expr>"))" | "("<expr>"?"<expr>":"<expr>")" | <terminal>
<terminal>                  ::= "a0" | "a1" | "a2" | "d0" | "d1" | "d2" | "d3" | "d4" | "d5" | "d6" | "d7" 
<newline>                   ::= "\n"
